  "description": "rebugger",
  "private": false,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "lint": "eslint --ext .js,.vue src",
    "build": "node build/build.js",
    "lib": "node build/build.lib.js",
    "build:em": "node build/build.em.js",
    "tsc": "tsc",
    "serve": "ts-node-dev --respawn --transpileOnly ./server/app.ts",
    "serve:node": "npm run tsc && node server/build/app.js",
    "serve:pm2": "npm run tsc && cross-env NODE_ENV=prod pm2 start server/build/app.js -i 2 --name rebugger-server -e log/error.log",
    "stop": "pm2 delete rebugger-server",
    "restart": "npm run stop && npm run serve:pm2"
  },
